# Environment variables for frontend Vue app

# Address of deployed PiDualTx smart contract
VUE_APP_CONTRACT_ADDRESS=0xYourSmartContractAddressHere

# API endpoint URLs for backend services
VUE_APP_API_RATE_SERVICE=https://api.pidualtx.com/rate-service
VUE_APP_API_AI_SERVICE=https://api.pidualtx.com/ai-service
VUE_APP_API_SMARTCONTRACT_SERVICE=https://api.pidualtx.com/smartcontract-service

# Web3 provider (e.g., Infura or local node)
VUE_APP_WEB3_PROVIDER=https://mainnet.infura.io/v3/your_infura_project_id

# Network Id (e.g., 1 for Ethereum mainnet, or your testnet)
VUE_APP_CHAIN_ID=1

# Feature toggles and flags
VUE_APP_ENABLE_ANALYTICS=true
VUE_APP_ENABLE_DEBUG=false
